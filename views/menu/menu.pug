extends ../layout

block content
    h1(class="mt-4") #{title}
    div(class="container-fluid border p-2" style="margin-top : 35px;")
        a(class="btn btn-primary float-end mb-2" href="group/add") Add Menu
        table(class="table table-sm table-bordered display compact table-striped" id="tableMenu")
            thead 
                tr
                    th(scope="col" class="text-center") No
                    th(scope="col") Menu 
                    th(scope="col") Menu Desc
                    th(scope="col") Url 
                    th(scope="col") Icon
                    th(scope="col") Order Menu
                    th(scope="col") Created By 
                    th(scope="col") Updated By
                    th(scope="col" class="text-center") Action
            tbody 
               
