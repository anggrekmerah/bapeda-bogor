extends ../layout

block content
    h1(class="mt-4") #{title}
    div(class="container-fluid border p-2" style="margin-top : 35px;")
        a(class="btn btn-primary float-end mb-2" href="/sitemap/add") Add Sitemap
        table(class="table table-sm table-bordered display compact table-striped" id="tableSitemap")
            thead 
                tr
                    th(scope="col" class="text-center") No
                    th(scope="col") Group Name 
                    th(scope="col") Ordered
                    th(scope="col") Created By 
                    th(scope="col") Updated By
                    th(scope="col" class="text-center") Action
            tbody 
               